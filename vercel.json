{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/assets/(.*)",
            "destination": "/student-activity/dist/assets/$1"
        },
        {
            "source": "/((?!api|assets|favicon.ico|vite.svg).*)",
            "destination": "/"
        }
    ]
}