{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.js"
        },
        {
            "source": "/assets/(.*)",
            "destination": "/student-activity/dist/assets/$1"
        },
        {
            "source": "/((?!api|assets).*\\.(?:css|js|png|jpg|jpeg|gif|svg|ico|json|txt|woff|woff2|ttf|eot))",
            "destination": "/student-activity/dist/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/student-activity/dist/index.html"
        }
    ]
}